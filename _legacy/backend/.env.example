# Snipe-IT Environment Configuration Example
APP_ENV=production
APP_DEBUG=false
APP_KEY=your-app-key-here
APP_URL=http://localhost:8080
APP_TIMEZONE=America/Chicago
APP_LOCALE=en

DB_CONNECTION=mysql
DB_HOST=snipe-it-db
DB_DATABASE=snipeit
DB_USERNAME=snipeit
DB_PASSWORD=your-db-password
DB_PORT=3306

MAIL_ENV_FROM_ADDR=your@email.com

# MySQL root password (not used by Snipe-IT, but required by MySQL)
MYSQL_ROOT_PASSWORD=your-root-password
